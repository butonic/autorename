<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>
	 <name>*dbname*</name>
	 <create>true</create>
	 <overwrite>false</overwrite>
	 <charset>utf8</charset>
	 <table>
		<name>*dbprefix*autorename</name>
		<declaration>
			<field>
				<name>userid</name>
				<type>text</type>
				<default></default>
				<notnull>true</notnull>
				<length>64</length>
			</field>
			<field>
				<name>sourceid</name>
				<type>integer</type>
				<default>0</default>
				<notnull>true</notnull>
				<length>4</length>
			</field>
			<field>
				<name>targetid</name>
				<type>integer</type>
				<default>0</default>
				<notnull>true</notnull>
				<length>4</length>
			</field>
			<index>
				<name>ar_index</name>
				<primary>true</primary>
				<unique>true</unique>
				<field>
					<name>userid</name>
					<sorting>ascending</sorting>
				</field>
				<field>
					<name>sourceid</name>
					<sorting>ascending</sorting>
				</field>
				<field>
					<name>targetid</name>
					<sorting>ascending</sorting>
				</field>
			</index>
		</declaration>
	</table>
</database>